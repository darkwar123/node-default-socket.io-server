{
  "name": "default-socket.io-server",
  "version": "1.0.2",
  "description": "Default Socket.io server",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkwar123/node-default-socket.io-server.git"
  },
  "scripts": {
    "dev": "set NODE_ENV=development && set DEBUG=* && set REDIS_URL=redis://127.0.0.1:6379 && node index.js",
    "prod": "set NODE_ENV=production && set REDIS_URL=redis://127.0.0.1:6379 && set DEBUG=*:info,*:error && node index.js"
  },
  "keywords": [
    "socket.io",
    "websocket",
    "http2"
  ],
  "author": "darkwar123",
  "license": "ISC",
  "dependencies": {
    "boom": "3.2.2",
    "debug": "^2.5.2",
    "flatten": "^1.0.2",
    "node-socket.io-router": "^1.1.3",
    "redis": "^2.8.0",
    "socket.io": "^2.1.1",
    "socket.io-redis": "^5.2.0"
  }
}
